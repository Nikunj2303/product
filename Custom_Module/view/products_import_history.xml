<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_import_history_form" model="ir.ui.view">
        <field name="name">products.import.history.form</field>
        <field name="model">products.import.history</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="product_template_id"/>
                    <field name="partner_id"/>
                    <field name="old_price"/>
                    <field name="new_price"/>
                </group>
            </form>
        </field>
    </record>

    <record id="view_product_import_history_tree" model="ir.ui.view">
        <field name="name">products.import.history.tree</field>
        <field name="model">products.import.history</field>
        <field name="arch" type="xml">
            <tree>
                <field name="product_template_id"/>
                <field name="partner_id"/>
                <field name="old_price"/>
                <field name="new_price"/>
            </tree>
        </field>
    </record>
</odoo>
